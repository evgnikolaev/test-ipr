{% extends "layouts::_page.twig" %}

{% block content %}


    {# field #}
    <div class="py-40">
        <div class="container">
            {% for field in fields %}
                <div class="py-8">
                    {% view "@field" with {
                        field:field
                    } %}
                </div>
            {% endfor %}
        </div>
    </div>


    {# field #}
    <div class="py-40 bg-blue">
        <div class="container">
            {% for field in fields2 %}
                <div class="py-8">
                    {% view "@field" with {
                        field:field
                    } %}
                </div>
            {% endfor %}
        </div>
    </div>


    {#link-action#}
    <div class="py-40">
        <div class="container">
            {% for link in links %}
                <div class="py-8">
                    {% view "@link-action" with {
                        link:link
                    } %}
                </div>
            {% endfor %}
        </div>
    </div>


    {#link-action#}
    <div class="py-40 bg-blue">
        <div class="container">
            {% for link in links2 %}
                <div class="py-8">
                    {% view "@link-action" with {
                        link:link
                    } %}
